                                <option value="outlet">Danh sách Outlet chưa có tọa độ</option>
                            }
@*@if (PermissionHelper.CheckPermissionByFeature("Home_ReportSalemanKPI"))
                            { 
                                <option value="ReportSalemanKPI">Báo cáo Tuân thủ - Chấm công</option>
                            }*@
