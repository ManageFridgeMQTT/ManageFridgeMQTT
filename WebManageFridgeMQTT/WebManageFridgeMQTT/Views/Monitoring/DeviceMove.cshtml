@model WebManageFridgeMQTT.Utility.DeviceActivity
<div class="row">
    <div class="item fl">
        <label class="fl">Chọn Ngày</label>
        @Html.TextBox("strFromDateHoatDong", Model.FromDate.ToShortDateString(), new { @class = "datepicker" })
        @Html.TextBox("strToDateHoatDong", Model.ToDate.ToShortDateString(), new { @class = "datepicker" })
    </div>
    <div id="btn-xemDiChuyen" class="item fl">
        <button onclick="RenderDiChuyen('@Model.ThietBiID')">
            <i class="icon-submit" title="Xem báo cáo"></i>Xem
        </button>
    </div>
</div>
<div class="row content-body">
    <div id="table-dichuyen" class="tab-content wrapper">
        <table class="tablesorter">
            <thead>
                <tr>
                    <th>Thời Gian</th>
                    <th>Vị trí</th>
                </tr>
            </thead>
            <tbody>
                @if (Model.ListDataMove != null)
                {
                    foreach (var item in Model.ListDataMove)
                    {
                        <tr>
                            <td>@item.Ngay.ToString()</td>
                            <td>@item.DenViTri</td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        $('.datepicker').datepicker({
            dateFormat: 'dd/mm/yy'
        });
    });
</script>